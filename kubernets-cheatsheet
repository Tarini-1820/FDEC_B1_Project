# kubernetes cmd 

kubectl cluster-info 

kubectl get nodes  # list of nodes 

kubectl get pod   # lists of pods 

kubectl create namespace dev  # create namespace 

kubectl get pods -n dev  

kubectl api-resources

kubectl delete pod podname 

kubectl delete namespace dev 

kubectl run my-app --image nginx --type NodePort --lebels app=my-app,env=dev --port=80

kubectl expose pod my-


